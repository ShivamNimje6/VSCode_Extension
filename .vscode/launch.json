{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/VSCode_Extension"
      ],
      "cwd": "${workspaceFolder}/VSCode_Extension",
      "outFiles": [
        "${workspaceFolder}/VSCode_Extension/out/**/*.js"
      ],
      "preLaunchTask": "npm: compile"
    }
  ]
}
